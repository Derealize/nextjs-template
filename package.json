{
  "name": "next-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write **/*.{js,jsx,ts,tsx} --config ./.prettierrc"
  },
  "dependencies": {
    "autoprefixer": "^10.2.4",
    "next": "10.0.6",
    "postcss": "^8.2.6",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "tailwindcss": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@derealize/babel-plugin-transform-react": "^1.0.7",
    "@tailwindcss/forms": "^0.3.2",
    "@tailwindcss/jit": "^0.1.18",
    "@types/node": "^14.14.28",
    "@types/react": "^17.0.2",
    "prettier": "^2.2.1",
    "typescript": "^4.1.5"
  },
  "derealize": {
    "branch": "derealize",
    "runScript": "dev",
    "formatScript": "format",
    "lunchUrl": "http://localhost:3000",
